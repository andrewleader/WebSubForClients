<Window x:Class="WebSubClientSampleApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebSubClientSampleApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBox
            x:Name="TextBoxSubscriptionUrl"
            Text="http://localhost:51867/api/subscriptions/tacos"
            KeyUp="TextBoxSubscriptionUrl_KeyUp"/>
        <TextBlock
            x:Name="TextBlockResults"
            Grid.Row="1"
            Margin="20"/>
    </Grid>
</Window>
